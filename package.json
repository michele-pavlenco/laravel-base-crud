    "dependencies": {
        "bootstrap": "^5.1.3",
        "global": "^4.4.0",
        "packages": "^0.0.8"
    },
    "devDependencies": {
        "axios": "^0.19",
        "cross-env": "^7.0",
        "laravel-mix": "^5.0.1",
        "lodash": "^4.17.13",
        "resolve-url-loader": "^3.1.0",
